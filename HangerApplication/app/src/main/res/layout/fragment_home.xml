<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#ffffff"
    tools:context="com.example.geehy.hangerapplication.Fragments.HomeFragment">

    <!-- TODO: Update blank fragment layout -->

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingActionButton"
        android:layout_width="48dp"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|right"
        android:layout_marginBottom="72dp"
        android:layout_marginRight="16dp"
        android:layout_weight="0.03"
        android:src="@drawable/ic_photo_camera"
        app:background="@color/colorPrimary"
        app:backgroundTint="#9848d1"
        app:elevation="5dp"
        app:fabSize="normal"
        app:rippleColor="#000000" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingButton_delete"
        android:layout_width="48dp"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|right"
        android:layout_marginBottom="132dp"
        android:layout_marginRight="16dp"
        android:layout_weight="0.03"
        android:src="@drawable/ic_delete_black_24dp"
        app:background="@color/colorPrimary"
        app:backgroundTint="#9848d1"
        app:elevation="5dp"
        app:fabSize="normal"
        app:rippleColor="#000000" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingButton_undo"
        android:layout_width="48dp"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|right"
        android:layout_marginBottom="72dp"
        android:layout_marginRight="16dp"
        android:layout_weight="0.03"
        android:src="@drawable/ic_reply_black_24dp"
        app:background="@color/colorPrimary"
        app:backgroundTint="#9848d1"
        app:elevation="5dp"
        app:fabSize="normal"
        app:rippleColor="#000000"
        android:visibility="gone"/>


    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1">


            <GridView
                android:id="@+id/home_gridview"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:background="#e7e7e7e7"
                android:columnWidth="100dp"
                android:horizontalSpacing="10dp"
                android:numColumns="2"
                android:padding="10dp"
                android:stretchMode="columnWidth"
                android:verticalSpacing="10dp"
                android:choiceMode="multipleChoice" />



            <LinearLayout
                android:id="@+id/home_insert_Layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_alignParentStart="true"
                android:background="#ffffff"
                android:orientation="horizontal"
                android:visibility="gone">

            </LinearLayout>
        </RelativeLayout>
    </LinearLayout>
</FrameLayout>