<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.geehy.hangerapplication.Fragments.DressFragment">


    <android.support.design.widget.FloatingActionButton
        android:id="@+id/dress_fbtn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|right"
        android:layout_marginBottom="72dp"
        android:layout_marginRight="20dp"
        android:layout_weight="0.03"
        android:src="@drawable/ic_add_black_24dp"
        app:background="@color/colorPrimary"
        app:backgroundTint="#0070C0"
        app:elevation="5dp"
        app:fabSize="normal"
        app:rippleColor="#000000" />
    <ImageView
        android:id="@+id/dashimgtemp1"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_gravity="bottom|right"
        android:layout_marginBottom="30dp"
        android:layout_marginRight="220dp"
        android:layout_weight="0.02"
        app:srcCompat="@drawable/tem3" />


    <LinearLayout
        android:id="@+id/dress_insert_Layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:background="#ffffff"
        android:orientation="horizontal"
        android:visibility="gone">

        <Button
            android:id="@+id/dash_closet_btn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:layout_weight="1"
            android:background="#0070C0"
            android:text="♡Add Clothes♡"
            android:textColor="#ffffff"
            android:textSize="20dp"
            android:visibility="visible" />

        <Button
            android:id="@+id/dash_coordy_btn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:layout_weight="1"
            android:background="#0070C0"
            android:text="♡Add Coordy♡"
            android:textColor="#ffffff"
            android:textSize="20dp"
            android:visibility="visible" />
    </LinearLayout>

    <CalendarView
        android:id="@+id/calendar"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</FrameLayout>
